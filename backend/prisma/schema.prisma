generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model producto {
  id            Int     @id
  nombre        String  @db.VarChar(50)
  descripcion   String
  clasificacion String  @db.VarChar(50)
  precio        Decimal @db.Decimal(10, 2)
}
